prod = []
pree = []
mrr = []
for url in URL:
    page = requests.get(url)
    soup = BeautifulSoup(page.content, 'html.parser')
    lists = soup.findAll('div',attrs={'class' :'_1AtVbE col-12-12'})             
 
    for list in lists:
        product = list.find(attrs={'class':["s1Q9rs","IRpwTa","_4rR01T"]}) 
        if product is not None:
            ementa = product.text
        else:
            ementa = None
        prod.append(ementa)
        sprice = list.find('div', attrs={'class':'_30jeq3'})
        if sprice is not None:
            sprr = sprice.text
        else:
            sprr = None
        pree.append(sprr)
        # pree.append(sprice)
        # print(price)
        mrp = list.find('div', attrs={'class':'_3I9_wc'})
        if mrp is not None:
            mpp = mrp.text
        else:
            mpp = None
        mrr.append(mpp)

        # mrr.append(mrp)

df = pd.DataFrame({'Product Name':prod,'Selling Price':pree, 'MRP':mrr}) 
df.to_csv('FKallbrands5.csv', index=False, encoding='utf-8') 
    
 
